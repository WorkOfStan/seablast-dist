{
    "name": "seablast/dist",
    "description": "Distribution of Seablast for PHP - a seed application",
    "type": "project",
    "require": {
        "php": "^7.2|^8.2",
        "robmorgan/phinx": "^0.10.6|^0.12.3",
        "seablast/seablast": "dev-mvc"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5|^6|^7|^8|^9|^10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Rejthar",
            "email": "rejthar@stanislavrejthar.com"
        }
    ],
    "support": {
        "email": "rejthar@stanislavrejthar.com",
        "issues": "https://github.com/WorkOfStan/seablast-dist/issues",
        "source": "https://github.com/WorkOfStan/seablast-dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seablast\\Distribution\\Models\\": ["models/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seablast\\Distribution\\Test\\": ["Test/"]
        }
    }
}
